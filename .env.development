# Development Environment Configuration
# Base URLs and Paths
PUBLIC_URL=
REACT_APP_PUBLIC_URL=http://localhost:3000
REACT_APP_BASE_PATH=/
REACT_APP_API_URL=http://localhost:5001

# Server Configuration
SERVER_PORT=5001
CLIENT_PORT=3000
HOST=localhost
HTTPS=false

# Data Paths
REACT_APP_DATA_PATH=/results
REACT_APP_ASSETS_PATH=/static/media

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_CHUNK_SIZE=500000
REACT_APP_CACHE_DURATION=3600000
REACT_APP_VERSION=$npm_package_version
REACT_APP_NAME=$npm_package_name
NODE_ENV=development
REACT_APP_ENABLE_REDUX_LOGGER=true

# Feature Flags
REACT_APP_ENABLE_DEBUG_LOGGING=true
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_SERVICE_WORKER=false

# Performance Settings
MINIMIZE_IN_DEVELOPMENT=false
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# Data Loading Configuration
REACT_APP_RETRY_ATTEMPTS=3
REACT_APP_RETRY_DELAY=1000
REACT_APP_CACHE_DURATION=300000
REACT_APP_PREFETCH_DATA=true